{
  "name": "gsl-interpreter",
  "private": true,
  "description": "Interpreter for GSL written in typescript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "gsl": "dist/bin.js"
  },
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --minify && tsup bin.ts --minify",
    "lint": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justindhillon/gsl.git"
  },
  "keywords": [
    "programming-language",
    "sign-language",
    "interpreter"
  ],
  "author": "Justin Dhillon",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/justindhillon/gsl/issues"
  },
  "homepage": "https://github.com/justindhillon/gsl#readme",
  "dependencies": {
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
